<PropertyList>
	
	<path>vsi.ac</path>
	
	<animation>
		<type>material</type>
		<object-name>VSIface</object-name>
		<object-name>VSI.needle</object-name>
		<emission>
			<red>0.3</red>
			<green>0.3</green>
			<blue>0.3</blue>
		</emission>
	</animation>
	
	<animation>
		<type>rotate</type>
		<object-name>VSI.needle</object-name>
		<property>instrumentation/vertical-speed-indicator/vert-speed-fpm</property>
		<interpolation>
			<entry><ind>-6000</ind><dep>-170</dep></entry>
			<entry><ind>-4000</ind><dep>-130</dep></entry>
			<entry><ind>-2000</ind><dep> -90</dep></entry>
			<entry><ind>-1000</ind><dep> -55</dep></entry>
			<entry><ind>  -10</ind><dep>   0</dep></entry>
			<entry><ind>   10</ind><dep>   0</dep></entry>
			<entry><ind> 1000</ind><dep>  55</dep></entry>
			<entry><ind> 2000</ind><dep>  90</dep></entry>
			<entry><ind> 4000</ind><dep> 130</dep></entry>
			<entry><ind> 6000</ind><dep> 170</dep></entry>
		</interpolation>
		<axis>
			<x>-1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>

</PropertyList>