<PropertyList>
	
	<path>tri.ac</path>
	
	<animation>
		<type>material</type>
		<object-name>tri</object-name>
		<object-name>toOff</object-name>
		<object-name>flxOff</object-name>
		<object-name>gaOff</object-name>
		<object-name>mctOff</object-name>
		<object-name>clOff</object-name>
		<object-name>crOff</object-name>
		<object-name>testBtn</object-name>
		<object-name>ratDigits</object-name>
		<object-name>eprDigits</object-name>
		<emission>
			<red>0.3</red>
			<green>0.3</green>
			<blue>0.3</blue>
		</emission>
	</animation>
	
	<animation>
		<type>pick</type>
		<object-name>toBtn</object-name>
		<action>
			<button>0</button>
			<repeatable>false</repeatable>
			<binding>
				<command>property-assign</command>
				<property>fdm/jsbsim/engine/limit/active-mode-int</property>
				<value>0</value>
			</binding>
			<binding>
				<command>property-assign</command>
				<property>controls/tri/to-btn</property>
				<value>1</value>
			</binding>
			<mod-up>
				<binding>
					<command>property-assign</command>
					<property>controls/tri/to-btn</property>
					<value>0</value>
				</binding>
			</mod-up>
		</action>
	</animation>
	
	<animation>
		<type>pick</type>
		<object-name>flxBtn</object-name>
		<action>
			<button>0</button>
			<repeatable>false</repeatable>
			<binding>
				<command>property-assign</command>
				<property>fdm/jsbsim/engine/limit/active-mode-int</property>
				<value>5</value>
			</binding>
			<binding>
				<command>property-assign</command>
				<property>controls/tri/flx-btn</property>
				<value>1</value>
			</binding>
			<mod-up>
				<binding>
					<command>property-assign</command>
					<property>controls/tri/flx-btn</property>
					<value>0</value>
				</binding>
			</mod-up>
		</action>
	</animation>
	
	<animation>
		<type>pick</type>
		<object-name>gaBtn</object-name>
		<action>
			<button>0</button>
			<repeatable>false</repeatable>
			<binding>
				<command>property-assign</command>
				<property>fdm/jsbsim/engine/limit/active-mode-int</property>
				<value>1</value>
			</binding>
			<binding>
				<command>property-assign</command>
				<property>controls/tri/ga-btn</property>
				<value>1</value>
			</binding>
			<mod-up>
				<binding>
					<command>property-assign</command>
					<property>controls/tri/ga-btn</property>
					<value>0</value>
				</binding>
			</mod-up>
		</action>
	</animation>
	
	<animation>
		<type>pick</type>
		<object-name>mctBtn</object-name>
		<action>
			<button>0</button>
			<repeatable>false</repeatable>
			<binding>
				<command>property-assign</command>
				<property>fdm/jsbsim/engine/limit/active-mode-int</property>
				<value>2</value>
			</binding>
			<binding>
				<command>property-assign</command>
				<property>controls/tri/mct-btn</property>
				<value>1</value>
			</binding>
			<mod-up>
				<binding>
					<command>property-assign</command>
					<property>controls/tri/mct-btn</property>
					<value>0</value>
				</binding>
			</mod-up>
		</action>
	</animation>
	
	<animation>
		<type>pick</type>
		<object-name>clBtn</object-name>
		<action>
			<button>0</button>
			<repeatable>false</repeatable>
			<binding>
				<command>property-assign</command>
				<property>fdm/jsbsim/engine/limit/active-mode-int</property>
				<value>3</value>
			</binding>
			<binding>
				<command>property-assign</command>
				<property>controls/tri/cl-btn</property>
				<value>1</value>
			</binding>
			<mod-up>
				<binding>
					<command>property-assign</command>
					<property>controls/tri/cl-btn</property>
					<value>0</value>
				</binding>
			</mod-up>
		</action>
	</animation>
	
	<animation>
		<type>pick</type>
		<object-name>crBtn</object-name>
		<action>
			<button>0</button>
			<repeatable>false</repeatable>
			<binding>
				<command>property-assign</command>
				<property>fdm/jsbsim/engine/limit/active-mode-int</property>
				<value>4</value>
			</binding>
			<binding>
				<command>property-assign</command>
				<property>controls/tri/cr-btn</property>
				<value>1</value>
			</binding>
			<mod-up>
				<binding>
					<command>property-assign</command>
					<property>controls/tri/cr-btn</property>
					<value>0</value>
				</binding>
			</mod-up>
		</action>
	</animation>
	
	<animation>
		<type>select</type>
		<object-name>toOn</object-name>
		<condition>
			<or>
				<equals>
					<property>fdm/jsbsim/engine/limit/active-mode-int</property>
					<value>0</value>
				</equals>
				<equals>
					<property>controls/switches/annun-test</property>
					<value>1</value>
				</equals>
			</or>
		</condition>
	</animation>
	
	<animation>
		<type>select</type>
		<object-name>flxOn</object-name>
		<condition>
			<or>
				<equals>
					<property>fdm/jsbsim/engine/limit/active-mode-int</property>
					<value>5</value>
				</equals>
				<equals>
					<property>controls/switches/annun-test</property>
					<value>1</value>
				</equals>
			</or>
		</condition>
	</animation>
	
	<animation>
		<type>select</type>
		<object-name>gaOn</object-name>
		<condition>
			<or>
				<equals>
					<property>fdm/jsbsim/engine/limit/active-mode-int</property>
					<value>1</value>
				</equals>
				<equals>
					<property>controls/switches/annun-test</property>
					<value>1</value>
				</equals>
			</or>
		</condition>
	</animation>
	
	<animation>
		<type>select</type>
		<object-name>mctOn</object-name>
		<condition>
			<or>
				<equals>
					<property>fdm/jsbsim/engine/limit/active-mode-int</property>
					<value>2</value>
				</equals>
				<equals>
					<property>controls/switches/annun-test</property>
					<value>1</value>
				</equals>
			</or>
		</condition>
	</animation>
	
	<animation>
		<type>select</type>
		<object-name>clOn</object-name>
		<condition>
			<or>
				<equals>
					<property>fdm/jsbsim/engine/limit/active-mode-int</property>
					<value>3</value>
				</equals>
				<equals>
					<property>controls/switches/annun-test</property>
					<value>1</value>
				</equals>
			</or>
		</condition>
	</animation>
	
	<animation>
		<type>select</type>
		<object-name>crOn</object-name>
		<condition>
			<or>
				<equals>
					<property>fdm/jsbsim/engine/limit/active-mode-int</property>
					<value>4</value>
				</equals>
				<equals>
					<property>controls/switches/annun-test</property>
					<value>1</value>
				</equals>
			</or>
		</condition>
	</animation>
	
	<animation>
		<type>translate</type>
		<object-name>toBtn</object-name>
		<property>controls/tri/to-btn</property>
		<factor>-0.002</factor>
		<axis>
			<x>1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>
	
	<animation>
		<type>translate</type>
		<object-name>flxBtn</object-name>
		<property>controls/tri/flx-btn</property>
		<factor>-0.002</factor>
		<axis>
			<x>1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>
	
	<animation>
		<type>translate</type>
		<object-name>gaBtn</object-name>
		<property>controls/tri/ga-btn</property>
		<factor>-0.002</factor>
		<axis>
			<x>1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>
	
	<animation>
		<type>translate</type>
		<object-name>mctBtn</object-name>
		<property>controls/tri/mct-btn</property>
		<factor>-0.002</factor>
		<axis>
			<x>1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>
	
	<animation>
		<type>translate</type>
		<object-name>clBtn</object-name>
		<property>controls/tri/cl-btn</property>
		<factor>-0.002</factor>
		<axis>
			<x>1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>
	
	<animation>
		<type>translate</type>
		<object-name>crBtn</object-name>
		<property>controls/tri/cr-btn</property>
		<factor>-0.002</factor>
		<axis>
			<x>1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>
	
	<!-- Indications -->
	<animation>
		<type>select</type>
		<object-name>ratPlus</object-name>
		<condition>
			<greater-than>
				<property>fdm/jsbsim/propulsion/tat-c</property>
				<value>-0.5</value>
			</greater-than>
		</condition>
	</animation>
	
	<animation>
		<type>select</type>
		<object-name>ratMinus</object-name>
		<condition>
			<less-than-equals>
				<property>fdm/jsbsim/propulsion/tat-c</property>
				<value>-0.5</value>
			</less-than-equals>
		</condition>
	</animation>
	
	<animation>
		<type>textranslate</type>
		<object-name>rat1</object-name>
		<property>controls/tri/rat-d10</property>
		<factor>0.0348</factor>
		<step>1</step>
		<axis>
			<x>0</x>
			<y>1</y>
			<z>0</z>
		</axis>
	</animation>
	
	<animation>
		<type>textranslate</type>
		<object-name>rat2</object-name>
		<property>controls/tri/rat-d1</property>
		<factor>0.0348</factor>
		<step>1</step>
		<axis>
			<x>0</x>
			<y>1</y>
			<z>0</z>
		</axis>
	</animation>
	
	<animation>
		<type>textranslate</type>
		<object-name>epr1</object-name>
		<property>controls/tri/limit-d100</property>
		<factor>0.0348</factor>
		<step>1</step>
		<axis>
			<x>0</x>
			<y>1</y>
			<z>0</z>
		</axis>
	</animation>
	
	<animation>
		<type>textranslate</type>
		<object-name>epr2</object-name>
		<property>controls/tri/limit-d10</property>
		<factor>0.0348</factor>
		<step>1</step>
		<axis>
			<x>0</x>
			<y>1</y>
			<z>0</z>
		</axis>
	</animation>
	
	<animation>
		<type>textranslate</type>
		<object-name>epr3</object-name>
		<property>controls/tri/limit-d1</property>
		<factor>0.0348</factor>
		<step>1</step>
		<axis>
			<x>0</x>
			<y>1</y>
			<z>0</z>
		</axis>
	</animation>

</PropertyList>
