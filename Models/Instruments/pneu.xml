<?xml version="1.0"?>

<PropertyList>

<path>pneu.ac</path>

<animation>
  <type>material</type>
  <object-name>Face</object-name>
  <object-name>Needle</object-name>
		<emission>
			<red>0.3</red>
			<green>0.3</green>
			<blue>0.3</blue>
		</emission>
</animation>

<animation>
  <type>rotate</type>
  <object-name>Needle</object-name>
  <property>systems/pneumatic/psi</property>
  <interpolation>
    <entry><ind>  0</ind><dep>  0</dep></entry>
    <entry><ind>120</ind><dep>360</dep></entry>
  </interpolation>
  <axis>
    <x>-1</x>
    <y>0</y>
    <z>0</z>
  </axis>
</animation>

</PropertyList>

